{
  "dashboard": {
    "id": null,
    "title": "n8n Data Intelligence Orchestrator - Comprehensive Dashboard",
    "tags": ["n8n", "data-orchestrator", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Total Executions (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "SELECT COUNT(*) as value FROM audit_logs WHERE start_time >= NOW() - INTERVAL '24 hours'",
            "datasource": "PostgreSQL-n8n",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 10
                },
                {
                  "color": "green",
                  "value": 50
                }
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 2,
        "title": "Success Rate Percentage",
        "type": "gauge",
        "targets": [
          {
            "expr": "SELECT ROUND((COUNT(CASE WHEN status = 'success' THEN 1 END)::DECIMAL / NULLIF(COUNT(*), 0) * 100), 2) as value FROM audit_logs WHERE start_time >= NOW() - INTERVAL '24 hours'",
            "datasource": "PostgreSQL-n8n",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "green",
                  "value": 90
                }
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 3,
        "title": "Workflow Execution Timeline",
        "type": "timeseries",
        "targets": [
          {
            "expr": "SELECT start_time as time, workflow_name, CASE WHEN status = 'success' THEN 1 ELSE 0 END as value FROM audit_logs WHERE start_time >= NOW() - INTERVAL '24 hours' ORDER BY start_time",
            "datasource": "PostgreSQL-n8n",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "always",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          }
        }
      },
      {
        "id": 4,
        "title": "Executions by Workflow Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "SELECT workflow_name, COUNT(*) as value FROM audit_logs WHERE start_time >= NOW() - INTERVAL '24 hours' GROUP BY workflow_name",
            "datasource": "PostgreSQL-n8n",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": []
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 5,
        "title": "Recent Data Versions",
        "type": "table",
        "targets": [
          {
            "expr": "SELECT version, record_count, file_size, created_at FROM data_versions ORDER BY created_at DESC LIMIT 10",
            "datasource": "PostgreSQL-n8n",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 9,
          "x": 6,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "file_size"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "bytes"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "created_at"
              },
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "auto"
                }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true
        }
      },
      {
        "id": 6,
        "title": "Active Alerts with Severity",
        "type": "table",
        "targets": [
          {
            "expr": "SELECT alert_type, severity, workflow_name, message, created_at, resolved FROM alerts WHERE created_at >= NOW() - INTERVAL '24 hours' ORDER BY CASE severity WHEN 'critical' THEN 1 WHEN 'high' THEN 2 WHEN 'medium' THEN 3 WHEN 'low' THEN 4 END, created_at DESC",
            "datasource": "PostgreSQL-n8n",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 9,
          "x": 15,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "mappings": [
              {
                "options": {
                  "critical": {
                    "color": "red",
                    "index": 0
                  },
                  "high": {
                    "color": "orange",
                    "index": 1
                  },
                  "medium": {
                    "color": "yellow",
                    "index": 2
                  },
                  "low": {
                    "color": "green",
                    "index": 3
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "severity"
              },
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true
        }
      },
      {
        "id": 7,
        "title": "Total Records Processed",
        "type": "stat",
        "targets": [
          {
            "expr": "SELECT COALESCE(SUM(records_processed), 0) as value FROM workflow_metrics WHERE created_at >= NOW() - INTERVAL '24 hours'",
            "datasource": "PostgreSQL-n8n",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 16
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 8,
        "title": "Workflow Performance Metrics (Avg Execution Time)",
        "type": "bargauge",
        "targets": [
          {
            "expr": "SELECT workflow_name, AVG(execution_time_ms) as avg_execution_time FROM workflow_metrics WHERE created_at >= NOW() - INTERVAL '24 hours' GROUP BY workflow_name",
            "datasource": "PostgreSQL-n8n",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 9,
          "x": 6,
          "y": 16
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 5000
                }
              ]
            },
            "unit": "ms"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "displayMode": "list",
          "showUnfilled": true
        }
      },
      {
        "id": 9,
        "title": "System Health Indicator",
        "type": "stat",
        "targets": [
          {
            "expr": "SELECT CASE WHEN success_rate_24h >= 90 AND unresolved_alerts = 0 THEN 1 WHEN success_rate_24h >= 70 THEN 0.5 ELSE 0 END as value FROM system_health",
            "datasource": "PostgreSQL-n8n",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 9,
          "x": 15,
          "y": 16
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "color": "red",
                    "index": 0,
                    "text": "🔴 Critical"
                  },
                  "0.5": {
                    "color": "yellow",
                    "index": 1,
                    "text": "🟡 Warning"
                  },
                  "1": {
                    "color": "green",
                    "index": 2,
                    "text": "🟢 Healthy"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.5
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "30s",
    "schemaVersion": 37,
    "version": 1,
    "links": [
      {
        "asDropdown": false,
        "icon": "external link",
        "includeVars": false,
        "keepTime": false,
        "tags": [],
