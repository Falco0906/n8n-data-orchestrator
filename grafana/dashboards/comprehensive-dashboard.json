        "title": "System Logs",
        "tooltip": "View system logs",
        "type": "link",
        "url": "http://localhost:5678/executions"
      }
    ],
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 0,
    "hideControls": false,
    "style": "dark",
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "datasource": "PostgreSQL-n8n",
          "definition": "SELECT DISTINCT workflow_name FROM audit_logs ORDER BY workflow_name",
          "hide": 0,
          "includeAll": true,
          "label": "Workflow",
          "multi": true,
          "name": "workflow",
          "options": [],
          "query": "SELECT DISTINCT workflow_name FROM audit_logs ORDER BY workflow_name",
          "refresh": 1,
