{
  "dashboard": {
    "id": null,
    "title": "n8n Data Orchestrator Overview",
    "tags": ["n8n", "orchestrator", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Workflow Execution Status",
        "type": "stat",
        "targets": [
          {
            "expr": "sum by (status) (n8n_workflow_executions_total)",
            "datasource": "PostgreSQL-n8n"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Data Processing Pipeline",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(n8n_workflow_executions_total[5m])",
            "datasource": "PostgreSQL-n8n"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Collector Status",
        "type": "singlestat",
        "targets": [
          {
            "expr": "n8n_workflow_status{workflow='collector'}",
            "datasource": "n8n-API"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 8
        },
        "valueName": "current"
      },
      {
        "id": 4,
        "title": "Validator Status",
        "type": "singlestat",
        "targets": [
          {
            "expr": "n8n_workflow_status{workflow='validator'}",
            "datasource": "n8n-API"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 8
        },
        "valueName": "current"
      },
      {
        "id": 5,
        "title": "Processor Status",
        "type": "singlestat",
        "targets": [
          {
            "expr": "n8n_workflow_status{workflow='processor'}",
            "datasource": "n8n-API"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 8
        },
        "valueName": "current"
      },
      {
        "id": 6,
        "title": "Reporter Status",
        "type": "singlestat",
        "targets": [
          {
            "expr": "n8n_workflow_status{workflow='reporter'}",
            "datasource": "n8n-API"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 8
        },
        "valueName": "current"
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "30s"
  }
}