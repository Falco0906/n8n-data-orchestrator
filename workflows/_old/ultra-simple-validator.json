{
  "name": "Ultra Simple Validator",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "validator",
        "responseMode": "responseNode"
      },
      "id": "webhook-validator",
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [300, 200]
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ JSON.stringify({\n  status: \"success\",\n  message: \"Validator working!\",\n  validation: {\n    isValid: true,\n    score: 0.95,\n    receivedData: $json\n  },\n  timestamp: new Date().toISOString()\n}) }}"
      },
      "id": "respond",
      "name": "Respond",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [500, 200]
    }
  ],
  "connections": {
    "Webhook": {
      "main": [
        [
          {
            "node": "Respond",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": false,
  "settings": {}
}